{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Typescript, React, Storybook, ui kit boilerplate",
  "scripts": {
    "build": "rollup -c --configPlugin typescript",
    "eslint": "eslint './**/*{ts,tsx,js}'",
    "stylelint": "stylelint 'src/**/*.css'",
    "tsc": "tsc --noEmit",
    "check": "npm run eslint && npm run stylelint && npm run tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "keywords": [
    "Typescript",
    "React",
    "Storybook",
    "UI kit template",
    "Modules",
    "Modular UI kit",
    "Tree shakable",
    "Rollup",
    "boilerplate",
    "template"
  ],
  "author": "Andrei Taranenko <i_oktav_i@mail.ru>",
  "license": "ISC",
  "browserslist": "> 2%, not dead",
  "exports": {
    "./base.css": "./build/styles/base.css",
    "./*": "./build/components/*/index.js",
    "./styles/*": "./build/components/*/index.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/components/*/index.d.ts"
      ]
    }
  },
  "files": [
    "./build/components",
    "./src/"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/webpack": "^5.28.0",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.4",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-mixins": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "rollup": "^2.61.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-htmlacademy": "^0.1.12",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-csstree-validator": "^1.9.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
